// Helper function to get environment variable with type safety
function getEnvVar(key) {
    return process.env[exports.ENV_VARS[key]];
}
// Helper function to get required environment variable
function getRequiredEnvVar(key) {
    const value = process.env[exports.ENV_VARS[key]];
    if (value === undefined) {
        throw new Error(`Required environment variable ${exports.ENV_VARS[key]} is not defined`);
    }
    return value;
}
// Helper function to get environment variable with default value
function getEnvVarWithDefault(key, defaultValue) {
    return process.env[exports.ENV_VARS[key]] || defaultValue;
}
