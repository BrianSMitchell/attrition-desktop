/**
 * Helper function to validate environment value
 */
function isValidEnvironment(env) {
    return Object.values(exports.ENV_VALUES).includes(env);
}
/**
 * Helper function to check if running in development
 */
function isDevelopmentEnv(env) {
    return env === exports.ENV_VALUES.DEVELOPMENT || env === exports.ENV_VALUES.LOCAL;
}
/**
 * Helper function to check if running in production
 */
function isProductionEnv(env) {
    return env === exports.ENV_VALUES.PRODUCTION;
}
/**
 * Helper function to check if running in test
 */
function isTestEnv(env) {
    return env === exports.ENV_VALUES.TEST;
}
